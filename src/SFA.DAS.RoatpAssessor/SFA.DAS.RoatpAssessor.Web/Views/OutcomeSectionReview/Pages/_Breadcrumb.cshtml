@model SFA.DAS.RoatpAssessor.Web.ViewModels.ApplicationSummaryAndBreadcrumbViewModel

<div class="govuk-breadcrumbs">
    <ol class="govuk-breadcrumbs__list">
        <li class="govuk-breadcrumbs__list-item">
            <a class="govuk-breadcrumbs__link" asp-action="Dashboard" asp-controller="Home">RoATP dashboard</a>
        </li>
        <li class="govuk-breadcrumbs__list-item">
            <a class="govuk-breadcrumbs__link" asp-action="Index" asp-controller="Home">RoATP assessor applications</a>
        </li>
        <li class="govuk-breadcrumbs__list-item">
            <a class="govuk-breadcrumbs__link" asp-action="ViewApplication" asp-controller="ModeratorOverview" asp-route-applicationId="@Model.ApplicationId">Application moderation overview</a>
        </li>
        <li class="govuk-breadcrumbs__list-item" aria-current="page">@Model.Caption</li>
    </ol>
</div>